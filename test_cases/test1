class test{
	
	public static void main(String[]args)
	{

		int [] [] foobar = 
		{
     		{10, 12, 14, 16, 18},
      		{20, 40, 40, 49, 18},
      		{4,3,1}
    	};

		int sum;	
   		int numrow;
   		int total;
   		int sumtotal;
		for (int row = 0; row < foobar.length; row++)
		{
		for (int col = 0; col < foobar[row].length; col++)
		{
			sum++
		}
		numrow++;
		sumtotal += sum;
		}
		total = sumtotal/numrow;
	}

	
}